<?xml version="1.0"?>
<launch>

  <arg name="robot" default="holonomic"/>
  <arg name="paused" default="false"/>
  <arg name="auto_quit" default="true"/>
  <arg name="use_sim_time" default="true"/>
  <arg name="gui" default="true"/>


 <!-- Launch PCL obstacle tracker -->
  <node pkg='kf_tracker_2' type='kf_tracker_2_node' name='kf_tracker_2'>
    <param name='obstacle_num' value='1'/>
    <param name='cluster_tolerance' value='0.15'/>
    <param name='min_cluster_size' value='5'/>
    <param name='max_cluster_size' value='300'/>
    <param name='voxel_leaf_size' value='0.1'/>

    <!-- Log scale hyper-parameter -->
    <param name='logSigma2_x' value='-1.4'/>
    <param name='logMagnSigma2_x' value='5.4'/>
    <param name='logLengthScale_x' value='-2.5'/>

    <param name='logSigma2_y' value='-1.2436'/>
    <param name='logMagnSigma2_y' value='-5.0423'/>
    <param name='logLengthScale_y' value='7.8164'/>

    <param name='data_length' value='30'/>
    <param name='param_fix' value='0'/>

    <remap from='input_pointcloud' to='/scan_matched_points2' />
    <!--remap from='input_pointcloud' to='/velodyne_points_front' /-->
  </node>

</launch>
